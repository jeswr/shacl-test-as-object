{
  "name": "shacl-test-as-object",
  "version": "1.0.0",
  "description": "Returns list of SHACL constraints from test suite as resources",
  "main": "dist/index.js",
  "types": "dist/index.d.js",
  "directories": {
    "lib": "lib"
  },
  "files": [
    "dist",
    "test-shapes"
  ],
  "scripts": {
    "test": "jest && npm run electron",
    "lint": "eslint lib/*.ts __tests__/*",
    "lint:fix": "eslint lib/*.ts __tests__/* --fix",
    "build:test": "tsc __tests__/integration-test.ts",
    "electron": "electron __tests__/integration-test.js",
    "build": "tsc",
    "prepare": "tsc",
    "semantic-release": "semantic-release"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jeswr/shacl-test-as-object.git"
  },
  "keywords": [
    "SHACL",
    "test",
    "resource"
  ],
  "author": "Jesse Wright <jesse.wright@anu.edu.au>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jeswr/shacl-test-as-object/issues"
  },
  "homepage": "https://github.com/jeswr/shacl-test-as-object#readme",
  "devDependencies": {
    "@types/electron": "^1.6.10",
    "@types/jest": "^26.0.20",
    "@types/n3": "^1.4.4",
    "@typescript-eslint/eslint-plugin": "^4.12.0",
    "@typescript-eslint/parser": "^4.12.0",
    "electron": "^11.1.1",
    "eslint": "^7.17.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.22.1",
    "jest": "^26.6.3",
    "pre-commit": "^1.2.2",
    "ts-jest": "^26.4.4",
    "typescript": "^4.1.5"
  },
  "dependencies": {
    "n3": "^1.7.0",
    "rdf-object": "^1.8.0",
    "rdf-object-proxy": "github:jeswr/rdf-object-proxy"
  },
  "jest": {
    "preset": "ts-jest",
    "collectCoverage": true,
    "modulePathIgnorePatterns": [
      "__tests__/integration-test.js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    }
  },
  "pre-commit": [
    "lint",
    "build",
    "test"
  ],
  "release": {
    "branches": [
      "main",
      "+([0-9])?(.{+([0-9]),x}).x",
      "next",
      {
        "name": "alpha",
        "prerelease": true
      },
      {
        "name": "beta",
        "prerelease": true
      }
    ]
  },
  "publishConfig": {
    "access": "public"
  }
}
